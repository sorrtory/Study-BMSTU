
(list->set '(1 1 2 3))                       ;; ⇒ (3 2 1)
;;(set? '(1 2 3))                              ;; ⇒ #t
;;(set? '(1 2 3 3))                            ;; ⇒ #f
;;(set? '())                                   ;; ⇒ #t
;;(union '(1 2 3) '(2 3 4))                    ;; ⇒ (4 3 2 1)
;;(intersection '(1 2 3) '(2 3 4))             ;; ⇒ (2 3)
;;(difference '(1 2 3 4 5) '(2 3))             ;; ⇒ (1 4 5)
;;(symmetric-difference '(1 2 3 4) '(3 4 5 6)) ;; ⇒ (6 5 2 1)
;;(set-eq? '(1 2 3) '(3 2 1))                  ;; ⇒ #t
;;(set-eq? '(1 2) '(1 3))                      ;; ⇒ #f